# Mist API Configuration
MIST_API_TOKEN=your_api_token_here
MIST_ORG_ID=your_org_id_here
MIST_API_HOST=api.mist.com

# Snowflake Configuration
SNF_ACCOUNT=your_snowflake_account
SNF_USER=your_snowflake_user
SNF_PASSWORD=your_snowflake_password
SNF_DATABASE=WAN_PERFORMANCE
SNF_SCHEMA=RETAIL_WAN
SNF_WAREHOUSE=COMPUTE_WH
SNF_ROLE=your_snowflake_role

# Threshold Configuration (percentages)
UTIL_THRESHOLD_WARN=70
UTIL_THRESHOLD_HIGH=80
UTIL_THRESHOLD_CRITICAL=90

# Quality Thresholds
LOSS_THRESHOLD_WARN=0.1
LOSS_THRESHOLD_HIGH=0.5
LOSS_THRESHOLD_CRITICAL=1.0

JITTER_THRESHOLD_WARN=10
JITTER_THRESHOLD_HIGH=30
JITTER_THRESHOLD_CRITICAL=50

LATENCY_THRESHOLD_WARN=50
LATENCY_THRESHOLD_HIGH=100
LATENCY_THRESHOLD_CRITICAL=150

# Operational Settings
PAGE_LIMIT=1000
RATE_LIMIT_DELAY=0.1
MAX_RETRIES=3
RETRY_DELAY=1.0

# Redis Caching Configuration
REDIS_URL=redis://localhost:6379
REDIS_ENABLED=true
REDIS_CACHE_TTL=300
REDIS_STALE_THRESHOLD=300

# Logging
LOG_LEVEL=INFO
LOG_FILE=data/logs/app.log
